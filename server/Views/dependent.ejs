<body  style="background:radial-gradient(rgb(162, 249, 255),rgb(245, 250, 248));">
    <h1 style="font-size: 400%;color:rgb(119, 9, 31);text-align: center; font-family:'Comic Neue';">DEPENDENT</h1>
    <table style="border: 2px solid;border-color:crimson;margin-left: 300px;margin-top:100px; font-family:'Comic Neue';">
        <tr>
            <th style="border:2px solid;font-size:200%;color:blue;border-color:crimson;">Essn</th>
            <th style="border:2px solid;font-size:200%;color:blue;border-color:crimson;">Name</th>
            <th style="border:2px solid;font-size:200%;color:blue;border-color:crimson;">Gender</th>
            <th style="border:2px solid;font-size:200%;color:blue;border-color:crimson;">Birth date</th>
        </tr>
 <% data.forEach(function(item, index){ %>
    <tr>
        <td style="border: 2px solid; padding: 2px;border-color:crimson;font-size:150%;"><%= item.essn %></td>
        <td style="border: 2px solid;  padding: 2px;border-color:crimson;font-size:150%;"><%= item.name %></td>
        <td style="border: 2px solid;  padding: 2px;border-color:crimson;font-size:150%;"><%= item.sex %></td>
        <td style="border: 2px solid;  padding: 2px;border-color:crimson;font-size:130%;"><%= item.bdate %></td>
        <!-- <td style="border: 2px solid;  padding: 2px;"><%= item.bdate %></td> -->
      
    </tr>
    <% }); %>
</table>
